- title @data_file.filename

= link_to "Back", data_files_path

= render_field "Name", @data_file.filename
= render_field "Date added", @data_file.created_at.to_s(:with_time)
= render_field "Added by", @data_file.created_by.email
= render_field "Start time", @data_file.start_time.to_s(:with_seconds) if @data_file.start_time
= render_field "End time", @data_file.end_time.to_s(:with_seconds) if @data_file.end_time