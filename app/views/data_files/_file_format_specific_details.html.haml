- if @data_file.format == "TOA5"
  - @data_file.metadata_key_value_pairs.each do |key, value|
    = render_field key.to_s.humanize, value

  %h3 Data File Columns
  %table#column_info
    %tr
      %th Column
      %th Unit
      %th Measurement Type
    - @data_file.column_details.each do |column_details|
      %tr
        %td= column_details.name
        %td= column_details.unit
        %td= column_details.data_type
